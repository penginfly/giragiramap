CREATE TABLE `kredo`.`employees` (`id` INT NOT NULL AUTO_INCREMENT , `first_name` VARCHAR(50) NOT NULL , `last_name` VARCHAR(50) NOT NULL , `salary` FLOAT(7,2) NOT NULL , `department` VARCHAR(50) NOT NULL , PRIMARY KEY (`id`))

INSERT INTO `employees` (`first_name`, `last_name`, `salary`, `department`) VALUES ('John', 'Davis', '20500', 'Adult'),('Emily', 'Jackson', '32000', 'Legal'),('Jack', 'Smith', '21000', 'Human Resources'), ('Janet', 'Jackson', '40500', 'IT'), ('Rose', 'Thampson', '36500',' Adult')

SELECT * FROM `employees`

SELECT `first_name`, `last_name` FROM `employees`

SELECT * FROM `employees` WHERE `first_name` = 'Jack'

SELECT * FROM `employees` WHERE id=3

DELETE FROM `employees` WHERE id = 1;
SELECT COUNT(*) FROM `employees`;

SELECT COUNT(department) FROM `employees` WHERE department = 'IT';

SELECT COUNT(first_name) FROM `employees` WHERE first_name = 'Jack';

SELECT MAX(salary) FROM `employees`;

SELECT `first_name`, `last_name` FROM `employees` WHERE salary = (SELECT MAX(salary) FROM `employees`);

SELECT `first_name`, `last_name` FROM `employees` WHERE salary = (SELECT MIN(salary) FROM `employees`);

SELECT * FROM `employees` ORDER BY salary DESC;